<div class="box <%= type %>">
	<span class="time"><%= object.created_at.strftime('%H:%M') %>h</span>
	
	<span class="image">
		<span class="label label-<%= color %>">
			<i class="<%= icon %> icon-white"></i> <%=t(type)%>
		</span>
	</span>

	<span class="text">
		<%= raw(text) %><b><%= link_to link , path %></b>
	</span>
</div>